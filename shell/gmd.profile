# --- Env. for IPC  ------------------------------------------
export BSCS_IPC_HOST_PORT=`echo 53000+$UID|bc`
export BSCS_IPC_PORT=`echo 54000+$UID*2|bc`

# --- Env. for SDP  ------------------------------------------
export GN_LOG_PATH=$BSCS_HOME/WORK/LOG/sdp/
export GN_LOG_LEVEL=5
export COUNTERPART_DATA_SERVER_WORKDIR=/tmp
export SVAPPLFILE=$BSCS_WORKDIR/CONF/application.ini

SG5INI=$BSCS_WORKDIR/CONF/sdp.ini
SDPINI=$SG5INI
PPCC_FTOK_DIR=$WORK/TMP
SDPUSER=$USER
PPCC_ENVIRONMENT_FILE=$HOME/ppcc_environment.ids
LOG_PATH=$WORK/LOG/sdp/
SDP_WORKDIR=$WORK
WORKDIR=$WORK
SV_ALARM_CONNECT=$BSCS_LIB/libppr_repm.so
SV_ALARM_PORT=`echo 55000+$UID|bc`
SV_SEM=0xBB800000
SV_KEY=0xBB800000

PPCC_ENVIRONMENT_ID=B

export SV_ALARM_CONNECT SV_ALARM_PORT SV_KEY SV_SEM SDP_WORKDIR WORKDIR
export PPCC_FTOK_DIR SDPINI SG5INI SDPUSER PPCC_ENVIRONMENT_ID PPCC_ENVIRONMENT_FILE LOG_PATH

# --- for SOP SNMP
export JSNMP_HOME=$BSCS_3PP/snmp-1.4
export SR_SNMP_TEST_PORT=`echo 57100+$UID|bc`
export SR_TRAP_TEST_PORT=`echo 57200+$UID|bc`
export SR_AGT_CONF_DIR=$WORK/SNMP
export SR_MGR_CONF_DIR=$WORK/SNMP
export SR_LOG_DIR=$WORK/SNMP
export DSNMP_HOME=$BSCS_3PP/snmp16.1.0.34
export SNMP_PORT=$SR_SNMP_TEST_PORT
export TRAP_PORT=$SR_TRAP_TEST_PORT

PATH=$DSNMP_HOME/bin:$PATH
PATH=/home/djabuco1/tools:$PATH
export NLS_LANG=AMERICAN_AMERICA.WE8ISO8859P1


# THIS ONE IS MANDATORY FOR GMD. ITLL NOT WORK WITHOUT IT
export GMD_NO_PROV_HANDSHAKE=1

